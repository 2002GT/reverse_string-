#include <iostream>

using namespace std;

//   -------------REVERSE THE STRING--------------------

int main()
{
   string str;
   cin>>str;
   string reverse_str;
   
for(int i = str.size()-1; i>=0; i--){
     reverse_str.push_back(str[i]);
}
    cout<<reverse_str<<endl;
    
 -----------checking for palindrome---------------
    if(str == reverse_str){
        cout<<"YES";
    }
     else{
         cout<<"NO";
     }
    
    return 0;
}
